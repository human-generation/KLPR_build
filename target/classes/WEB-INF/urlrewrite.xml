		<from>/</from>
		<to>/main.do</to>
	</rule>
	<rule>
		<from>/main</from>
		<to>/main.do</to>
	</rule>
	<rule>
		<from>/login</from>
		<to>/login.do</to>
	</rule>
	<rule>
		<from>/logout</from>
		<to>/logout.do</to>
	</rule>
	<rule>
		<from>/join</from>
		<to>/join.do</to>
	</rule>
</urlrewrite>